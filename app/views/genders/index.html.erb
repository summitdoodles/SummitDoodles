<div class="pagenav">
  <%= link_to 'New Gender', new_gender_path %>
</div>

<h1>Listing genders</h1>

<table>
  <tr>
    <th>Gender</th>
    <th></th>
    <th></th>
  </tr>

<% @genders.each do |gender| %>
  <tr>
    <td><%= gender.gender %></td>
    <td><%= link_to 'Edit', edit_gender_path(gender) %></td>
    <td><%= link_to 'Destroy', gender, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />


